{
  "name": "quicknode",
  "version": "0.0.1",
  "description": "QuickNode - an app for integrating quickbooks and salesforce",
  "main": "app.js",
  "scripts": {
    "test": "mocha",
    "build": "npm run build-clean && npm run build-sass && npm run build-autoprefix",
    "build-autoprefix": "postcss --use autoprefixer --map false --output css/bulma.css css/bulma.css",
    "build-clean": "rimraf css",
    "build-sass": "node-sass --output-style expanded --source-map true sass/custom-bulma.sass css/bulma.css",
    "start": "npm run build-sass --watch"
  },
  "keywords": [
    "Intuit",
    "QuickBooks",
    "IPP"
  ],
  "author": "Dan Rauch",
  "license": "Referenced as LICENSE.md",
  "dependencies": {
    "body-parser": "^1.18.3",
    "build": "^0.1.4",
    "bulma": "^0.7.1",
    "cookie-parser": "^1.4.3",
    "csrf": "^3.0.6",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "fontawesome": "^4.7.2",
    "jsforce": "^1.8.4",
    "jxon": "^2.0.0-beta.5",
    "moment": "^2.22.2",
    "nodemon": "^1.17.5",
    "pg": "^7.4.3",
    "querystring": "0.2.0",
    "request": "2.83.0",
    "request-debug": "0.2.0",
    "rifraf": "^2.0.3",
    "underscore": "1.6.0",
    "util": "0.10.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "async": "0.9.0",
    "bluebird": "^3.5.1",
    "expect": "0.1.1",
    "mocha": "2.2.5",
    "ramda": "0.17.1",
    "autoprefixer": "^7.1.1",
    "node-sass": "^4.7.2",
    "postcss-cli": "^4.1.0",
    "rimraf": "^2.6.1"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danjrauch/quicknode.git"
  },
  "bugs": {}
}
